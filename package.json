{
    "name": "ouryard-demo",
    "version": "1.0.0",
    "type": "module",
    "engines": {
        "node": ">=20.18.0"
    },
    "scripts": {
        "dev:frontend": "vite",
        "dev:backend": "nodemon --watch src/server.ts --exec node --loader ts-node/esm src/server.ts",
        "dev": "concurrently \"yarn dev:frontend\" \"yarn dev:backend\"",
        "build:server": "tsc -p tsconfig.server.json",
        "build:client": "vite build",
        "build": "yarn build:server && yarn build:client",
        "start": "node dist/server.js",
        "preview": "vite preview"
    },
    "dependencies": {
        "@types/leaflet": "^1.9.12",
        "@types/three": "^0.170.0",
        "antd": "^5.6.3",
        "essentia.js": "^0.1.3",
        "express": "^4.18.2",
        "leaflet": "^1.9.4",
        "phaser": "^3.87.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-leaflet": "^4.2.1",
        "three": "^0.170.0",
        "web-audio-beat-detector": "^8.2.18"
    },
    "devDependencies": {
        "@biomejs/biome": "1.9.3",
        "@types/express": "^4.17.17",
        "@types/node": "^20.3.1",
        "@types/react": "^18.2.14",
        "@types/react-dom": "^18.2.6",
        "@vitejs/plugin-react": "^4.0.1",
        "concurrently": "^8.2.0",
        "nodemon": "^2.0.22",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3",
        "vite": "^4.3.9",
        "vite-plugin-style-import": "^2.0.0"
    }
}
